<%=form_for [@security_level,@page] do |form| %>
	<div class="form-group">
      <%= form.label :controller_name %>
      <%= form.text_field :controller_name, :autofocus => true, class: 'form-control' %>
    </div>

    <div class="form-group">
      <%= form.label :action_name %>
      <%= form.text_field :action_name, :autofocus => true, class: 'form-control' %>
    </div>
    
    <%= form.submit 'Add', :class => 'button right' %>
<%end%>