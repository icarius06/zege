<%= form_for @permission do |form| %>


    <div class="form-group">
      <%= form.label :user_id %>
       <%= select :permission, :user_id,User.all.map {|c| [c.email,c.id]},:class=>"form-control"%>
    </div>

	<div class="form-group">
      <%= form.label :level_id %>
      <%= select :permission, :level_id,SecurityLevel.all.map {|c| [c.level,c.id]},:class=>"form-control"%>
	    
    </div>

    <%= form.submit 'Add', :class => 'button right' %>
<% end %>